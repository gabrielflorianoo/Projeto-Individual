generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model users {
  id      Int      @id @default(autoincrement())
  name    String?  @db.VarChar(30)
  pass    String?  @db.VarChar(30)
  email   String?  @db.VarChar(30)
  isAdmin Boolean? @default(dbgenerated("(false)"))
}
